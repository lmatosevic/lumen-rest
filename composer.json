{
  "name": "lujo/lumen-rest",
  "description": "Lumen package for creating simple REST API",
  "type": "library",
  "version": "2.0.0",
  "keywords": [
    "Lumen",
    "REST",
    "API"
  ],
  "require": {
    "php": ">=8.0.0",
    "laravel/lumen-framework": "^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1",
    "orchestra/testbench": "^v8.13"
  },
  "autoload": {
    "psr-4": {
      "Lujo\\Lumen\\Rest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lujo\\Lumen\\Rest\\Test\\": "tests"
    }
  },
  "authors": [
    {
      "name": "luka",
      "email": "lukamatosevic5@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "Lujo\\Lumen\\Rest\\LumenRestServiceProvider"
      ]
    }
  }
}
